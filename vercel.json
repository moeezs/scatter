{
  "rewrites": [
    {
      "source": "/reelax",
      "destination": "https://reelax-tau.vercel.app/"
    },
    {
      "source": "/reelax/:path*",
      "destination": "https://reelax-tau.vercel.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/reelax/:path*",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self'"
        }
      ]
    }
  ]
}